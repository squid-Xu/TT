{
	"pages": [
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/count/index",
			"style": {
				"navigationBarTitleText": "计数"
			}
		},
		{
			"path": "pages/record/index",
			"style": {
				"navigationBarTitleText": "记录"
			}
		}
	],
	"tabBar": {
		"color": "#909399",
		"selectedColor": "#29CCCC",
		"borderStyle": "black",
