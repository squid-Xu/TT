		"list": [
			{
				"pagePath": "pages/count/index",
				"iconPath": "static/count.png",
				"selectedIconPath": "static/count-select.png",
				"text": "计数"
			},
			{
				"pagePath": "pages/record/index",
				"iconPath": "static/record.png",
				"selectedIconPath": "static/record-select.png",
				"text": "记录"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "计数器",
		"navigationBarBackgroundColor": "#ffffff",
		"backgroundColor": "#F8F8F8"
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
		}
